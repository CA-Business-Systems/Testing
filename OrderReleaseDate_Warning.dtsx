        <SQLTask:SqlTaskData
          SQLTask:Connection="{699F9E7F-F62B-4B34-8A55-E93DF4D5CF75}"
          SQLTask:SqlStatementSource="with cte as ( &#xA;(Select soh.Name, soh.rsca_Released_Date__c, t.rstk__sootype_desc__c , soh.createddate , soh.rstk__sohdr_totamt__c, soh.rsca_Customer_Code__c, c.rstk__socust_sf_account_Name__c&#xA;From rstk__sohdr__c soh &#xA;Left join rstk__sootype__c t on soh.rstk__sohdr_otype__c = t.id &#xA;Left Join rstk__socust__c c on soh.rstk__sohdr_custno__c = c.id&#xA;where soh.rsca_Release_Sales_Order__c = 'true' and soh.rsca_Released_Date__c IS NULL)&#xA;union all &#xA;(Select soh.Name, soh.rsca_Released_Date__c, t.rstk__sootype_desc__c , soh.createddate , soh.rstk__sohdr_totamt__c, soh.rsca_Customer_Code__c, c.rstk__socust_sf_account_Name__c&#xA;From rstk__sohdr__c soh &#xA;Left join rstk__sootype__c t on soh.rstk__sohdr_otype__c = t.id &#xA;Left Join rstk__socust__c c on soh.rstk__sohdr_custno__c = c.id&#xA;where soh.rsca_Release_Sales_Order__c = 'false' and soh.rsca_Released_Date__c IS Not NULL) &#xA;)&#xA;Select Count(*) as TotCount from cte"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="TotCount"
