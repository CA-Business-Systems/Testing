                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- 2020.08.05 - ERP Release

with cte as ( 
(Select soh.Name as OrderNo
, soh.rsca_Released_Date__c as ReleaseDate
, t.rstk__sootype_desc__c as OrderType
, soh.createddate as CreateDate
, soh.rstk__sohdr_totamt__c as OrderTotal
, soh.rsca_Customer_Code__c as CustNo
, c.rstk__socust_sf_account_Name__c as CustName
, 'Released Order has no ReleaseDate' as Status
From rstk__sohdr__c soh 
Left join rstk__sootype__c t on soh.rstk__sohdr_otype__c = t.id 
Left Join rstk__socust__c c on soh.rstk__sohdr_custno__c = c.id
where soh.rsca_Release_Sales_Order__c = 'true' and soh.rsca_Released_Date__c IS NULL)

union all 

(Select soh.Name as OrderNo
, soh.rsca_Released_Date__c as ReleaseDate
, t.rstk__sootype_desc__c as OrderType
 , soh.createddate  as CreateDate
 , soh.rstk__sohdr_totamt__c as OrderTotal
 , soh.rsca_Customer_Code__c as CustNo
 , c.rstk__socust_sf_account_Name__c as CustName
 , 'Release Date on Order that is not Released' as Ststus
From rstk__sohdr__c soh 
Left join rstk__sootype__c t on soh.rstk__sohdr_otype__c = t.id 
Left Join rstk__socust__c c on soh.rstk__sohdr_custno__c = c.id
where soh.rsca_Release_Sales_Order__c = 'false' and soh.rsca_Released_Date__c IS Not NULL) 
)
 
 Select * from cte</property>
